{
  "name": "TKRClient",
  "description": "TKRClient extension is a script to modify the website https://iogames.fun.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "active.html",
    "default_icon": "favicon.png"
  },
  "content_scripts": [
    {
      "matches": [
      "http://iogames.fun/",
      "https://iogames.fun/"],
      "js": ["script.js"]
    }
  ]
}
